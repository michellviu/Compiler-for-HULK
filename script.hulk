    {
        print("Par");
    }
    elif(x%2==1)
    {
        print("Impar");
    }
    else
      print("No");
    while(x<=10)
    {
        print(x);
        x:=x+1;
    }
};

function operate:number(x:number, y:number) {
    print(x + y);
    print(x - y);
    print(x * y);
    print(x / y);
};

function suma:number(x:number,y:number) => x+y;;

operate(suma(2,3),3)


